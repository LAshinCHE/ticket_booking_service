{
  "dashboard": {
    "id": null,
    "uid": "pflgxl3vd",
    "title": "Saga\u2011Service Metrics",
    "tags": [
      "saga",
      "otel",
      "service"
    ],
    "timezone": "browser",
    "schemaVersion": 38,
    "version": 1,
    "refresh": "5s",
    "panels": [
      {
        "type": "timeseries",
        "title": "Saga Started / Succeeded / Failed (rate per min)",
        "datasource": "$DS_PROM",
        "targets": [
          {
            "expr": "rate(saga_started_total{service_name=\"$service\"}[1m])",
            "legendFormat": "started"
          },
          {
            "expr": "rate(saga_succeeded_total{service_name=\"$service\"}[1m])",
            "legendFormat": "succeeded"
          },
          {
            "expr": "rate(saga_failed_total{service_name=\"$service\"}[1m])",
            "legendFormat": "failed"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "id": 1
      },
      {
        "type": "gauge",
        "title": "Saga Success Rate (%)",
        "datasource": "$DS_PROM",
        "options": {
          "min": 0,
          "max": 100
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent"
          }
        },
        "targets": [
          {
            "expr": "100 * (saga_succeeded_total{service_name=\"$service\"} / saga_started_total{service_name=\"$service\"})"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 8
        },
        "id": 2
      },
      {
        "type": "timeseries",
        "title": "Activity Succeeded (rate per min)",
        "datasource": "$DS_PROM",
        "targets": [
          {
            "expr": "rate(saga_activity_total{activity=\"booking\",status=\"succeeded\"}[1m])",
            "legendFormat": "booking"
          },
          {
            "expr": "rate(saga_activity_total{activity=\"ticket\",status=\"succeeded\"}[1m])",
            "legendFormat": "ticket"
          },
          {
            "expr": "rate(saga_activity_total{activity=\"payment\",status=\"succeeded\"}[1m])",
            "legendFormat": "payment"
          },
          {
            "expr": "rate(saga_activity_total{activity=\"notification\",status=\"succeeded\"}[1m])",
            "legendFormat": "notification"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 14
        },
        "id": 3
      },
      {
        "type": "barGauge",
        "title": "Activity Failures (last 5m)",
        "datasource": "$DS_PROM",
        "targets": [
          {
            "expr": "increase(saga_activity_total{activity=\"booking\",status=\"failed\"}[5m])",
            "legendFormat": "booking"
          },
          {
            "expr": "increase(saga_activity_total{activity=\"ticket\",status=\"failed\"}[5m])",
            "legendFormat": "ticket"
          },
          {
            "expr": "increase(saga_activity_total{activity=\"payment\",status=\"failed\"}[5m])",
            "legendFormat": "payment"
          },
          {
            "expr": "increase(saga_activity_total{activity=\"notification\",status=\"failed\"}[5m])",
            "legendFormat": "notification"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 8,
          "y": 8
        },
        "id": 4
      },
      {
        "type": "timeseries",
        "title": "Activity Latency (avg ms)",
        "datasource": "$DS_PROM",
        "targets": [
          {
            "expr": "rate(saga_activity_latency_ms_sum{activity=\"booking\"}[1m]) / rate(saga_activity_latency_ms_count{activity=\"booking\"}[1m])",
            "legendFormat": "booking"
          },
          {
            "expr": "rate(saga_activity_latency_ms_sum{activity=\"ticket\"}[1m]) / rate(saga_activity_latency_ms_count{activity=\"ticket\"}[1m])",
            "legendFormat": "ticket"
          },
          {
            "expr": "rate(saga_activity_latency_ms_sum{activity=\"payment\"}[1m]) / rate(saga_activity_latency_ms_count{activity=\"payment\"}[1m])",
            "legendFormat": "payment"
          },
          {
            "expr": "rate(saga_activity_latency_ms_sum{activity=\"notification\"}[1m]) / rate(saga_activity_latency_ms_count{activity=\"notification\"}[1m])",
            "legendFormat": "notification"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 22
        },
        "id": 5
      }
    ],
    "templating": {
      "list": [
        {
          "type": "datasource",
          "name": "DS_PROM",
          "label": "Datasource",
          "query": "prometheus",
          "refresh": 1
        },
        {
          "name": "service",
          "label": "Service name",
          "type": "query",
          "datasource": "$DS_PROM",
          "refresh": 1,
          "query": "label_values(saga_started_total, service_name)"
        }
      ]
    }
  },
  "overwrite": false
}